// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlite"
  url      = "file:./db.sqlite"
}

model Guild {
  id     String  @id
  prefix String?
}

model User {
  id     String @id
  wallet Int    @default(0)
  bank   Int    @default(0)
}
